{
  "name": "bank-scraper",
  "version": "1.0.0",
  "description": "A bank scaper app",
  "main": "build/src/server.js",
  "scripts": {
    "test": "npm run build && cross-env NODE_ENV=test PORT=8080 mocha build/tests/*.test.js --exit",
    "lint": "eslint . --ext .ts",
    "clean": "rm -rf build",
    "build": "npm run clean && tsc",
    "forever": "forever build/src/server.js",
    "watch": "npm run build && concurrently -kr 'tsc --watch' 'nodemon --inspect --delay 1000ms'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kenware/bank-scraper.git"
  },
  "author": "kevin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kenware/bank-scraper/issues"
  },
  "homepage": "https://github.com/kenware/bank-scraper#readme",
  "dependencies": {
    "app-module-path": "^2.2.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "forever": "^4.0.0",
    "lodash": "^4.17.21",
    "mongoose": "^5.12.13",
    "morgan": "^1.10.0",
    "normalize-mongoose": "0.0.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/chai": "^4.2.18",
    "@types/cors": "^2.8.10",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.12",
    "@types/lodash": "^4.14.170",
    "@types/mocha": "^8.2.2",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.2",
    "@types/nock": "^11.1.0",
    "@types/node": "^15.12.1",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "concurrently": "^6.2.0",
    "eslint": "^7.28.0",
    "mocha": "^8.4.0",
    "nock": "^13.1.0",
    "nodemon": "^2.0.7",
    "typescript": "^4.3.2"
  }
}
